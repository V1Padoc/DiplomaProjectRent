{
  "development": {
    "username": "YOUR_LOCAL_DB_USER",
    "password": "YOUR_LOCAL_DB_PASSWORD",
    "database": "YOUR_LOCAL_DB_NAME",
    "host": "127.0.0.1",
    "dialect": "mysql"
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  }
}